{
  "name": "llm-code-highlighter",
  "dockerFile": "Dockerfile",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "dbaeumer.vscode-eslint",
        "ms-vscode.vscode-typescript-tslint-plugin",
        "esbenp.prettier-vscode",
        "eamodio.gitlens",
        "redhat.vscode-yaml",
        "sourcegraph.cody-ai"
      ]
    }
  },
  "remoteUser": "node"
}
